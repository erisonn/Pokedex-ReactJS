DetailsRequest\"\nimport { NavLink } from \"react-router-dom\"\nimport './ListItem.css'\n\nconst ListItem = ({itemURL}) => {\n\n    const { itemData, itemLink, isLoading } = useDetailsRequest(itemURL)\n\n    return (\n        <NavLink to={itemLink} className='list-link'>\n        <div className=\"list-item\">\n            <div className=\"item-image\">\n                <img src={itemData.img} alt=\"\"/>\n            </div>\n            <div className=\"item-name\">\n                <h1>{itemData.name}</h1>\n            </div>\n        </div>\n        </NavLink>\n    );\n}\n \nexport default ListItem;","/home/erison/Documents/Projetos/Pokedex-ReactJS/src/components/Loading/Loading.jsx",[],"/home/erison/Documents/Projetos/Pokedex-ReactJS/src/components/Error/Error.jsx",[],"/home/erison/Documents/Projetos/Pokedex-ReactJS/src/components/Button/Button.jsx",[],"/home/erison/Documents/Projetos/Pokedex-ReactJS/src/components/Header/Header.jsx",[],"/home/erison/Documents/Projetos/Pokedex-ReactJS/src/pages/Pokemon.js",["74"],"/home/erison/Documents/Projetos/Pokedex-ReactJS/src/hooks/useApiRequest.js",["75"],"import { useState, useEffect } from \"react\"\n\nconst useApiRequest = url => {\n\n    const [next, setNext] = useState(null)\n    const [isLoading, setIsLoading] = useState(true)\n    const [error, setError] = useState(null)\n    const [pokemons, setPokemons] = useState([])\n\n    const loadPokemons = () => {\n        if(error) {\n            setError(null)\n            setIsLoading(true)\n        }\n        fetch(url)\n        .then(response => response.json())\n        .then(PokemonData => {\n            setNext(PokemonData.next)\n            if (PokemonData.previous === null) {\n                setPokemons(PokemonData.results)\n            } else {\n                setPokemons([...pokemons, ...PokemonData.results])\n            }\n        })\n        .catch(error => {\n            console.log(error)\n            setError('Error on load.')\n        })\n        .finally(() => {\n            setIsLoading(false)\n        })\n    }\n\n    useEffect(() => {\n        setIsLoading(true)\n\n        loadPokemons()\n    }, [url])\n\n    return {next, isLoading, error, pokemons, loadPokemons }\n\n}\n\nexport default useApiRequest;","/home/erison/Documents/Projetos/Pokedex-ReactJS/src/hooks/useDetailsRequest.js",["76"],"/home/erison/Documents/Projetos/Pokedex-ReactJS/src/components/Tags/Tags.jsx",[],"/home/erison/Documents/Projetos/Pokedex-ReactJS/src/components/TagItem/TagItem.jsx",[],"/home/erison/Documents/Projetos/Pokedex-ReactJS/src/utils/helpers.js",[],{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","replacedBy":"80"},{"ruleId":"81","severity":1,"message":"82","line":1,"column":17,"nodeType":"83","messageId":"84","endLine":1,"endColumn":26},{"ruleId":"81","severity":1,"message":"85","line":7,"column":33,"nodeType":"83","messageId":"84","endLine":7,"endColumn":42},{"ruleId":"86","severity":1,"message":"87","line":26,"column":17,"nodeType":"88","endLine":26,"endColumn":43},{"ruleId":"89","severity":1,"message":"90","line":38,"column":8,"nodeType":"91","endLine":38,"endColumn":13,"suggestions":"92"},{"ruleId":"89","severity":1,"message":"93","line":43,"column":8,"nodeType":"91","endLine":43,"endColumn":13,"suggestions":"94"},"no-native-reassign",["95"],"no-negated-in-lhs",["96"],"no-unused-vars","'useEffect' is defined but never used.","Identifier","unusedVar","'isLoading' is assigned a value but never used.","jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'loadPokemons'. Either include it or remove the dependency array.","ArrayExpression",["97"],"React Hook useEffect has a missing dependency: 'loadDetails'. Either include it or remove the dependency array.",["98"],"no-global-assign","no-unsafe-negation",{"desc":"99","fix":"100"},{"desc":"101","fix":"102"},"Upda