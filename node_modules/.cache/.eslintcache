/src/Router.js",[],"/home/erison/Documents/Projetos/Pokedex-ReactJS/src/pages/Home.js",["60"],"import React, { useEffect, useState } from \"react\";\nimport useApiRequest from '../components/hooks/useApiRequest'\nimport List from \"../components/List/List\";\nimport Loading from \"../components/Loading/Loading\";\nimport Error from \"../components/Error/Error\";\nimport Button from \"../components/Button/Button\";\n\nconst Home = () => {\n\n    const [url, setUrl] = useState('https://pokeapi.co/api/v2/pokemon?limit=20')\n    const {next, isLoading, error, pokemons, loadPokemons } = useApiRequest(url)\n   \n    const loadMore = () => {\n        setUrl(next)\n    }\n\n    if(error) {\n        return (<Error errorMessage={error} handleError={loadPokemons}/>)\n    }\n\n    return ( \n        <div className='Home'>\n            {isLoading && <Loading/>}\n            <List data={pokemons}/>\n            {isLoading || <Button buttonText={'Load more.'} handleClick={loadMore} />}\n        </div>\n    );\n}\n \nexport default Home;","/home/erison/Documents/Projetos/Pokedex-ReactJS/src/components/List/List.jsx",[],"/home/erison/Documents/Projetos/Pokedex-ReactJS/src/components/ListItem/ListItem.jsx",["61"],"import useDetailsRequest from \"../hooks/useDetailsRequest\"\nimport { NavLink } from \"react-router-dom\"\nimport './ListItem.css'\n\nconst ListItem = ({itemURL}) => {\n\n    const { itemData, itemLink, isLoading } = useDetailsRequest(itemURL)\n\n    return (\n        <NavLink to={itemLink} className='list-link'>\n        <div className=\"list-item\">\n            <div className=\"item-image\">\n                <img src={itemData.img} alt=\"\"/>\n            </div>\n            <div className=\"item-name\">\n                <h1>{itemData.name}</h1>\n            </div>\n        </div>\n        </NavLink>\n    );\n}\n \nexport default ListItem;","/home/erison/Documents/Projetos/Pokedex-ReactJS/src/components/Loading/Loading.jsx",[],"/home/erison/Documents/Projetos/Pokedex-ReactJS/src/components/Error/Error.jsx",[],"/home/erison/Documents/Projetos/Pokedex-ReactJS/src/components/Button/Button.jsx",[],"/home/erison/Documents/Projetos/Pokedex-ReactJS/src/components/Header/Header.jsx",[],"/home/erison/Documents/Projetos/Pokedex-ReactJS/src/pages/Pokemon.js",["62"],"/home/erison/Documents/Projetos/Pokedex-ReactJS/src/components/hooks/useDetailsRequest.js",["63"],"/home/erison/Documents/Projetos/Pokedex-ReactJS/src/components/hooks/useApiRequest.js",["64"],"import { useState, useEffect } from \"react\"\n\nconst useApiRequest = url => {\n\n    const [next, setNext] = useState(null)\n    const [isLoading, setIsLoading] = useState(true)\n    const [error, setError] = useState(null)\n    const [pokemons, setPokemons] = useState([])\n\n    const loadPokemons = () => {\n        if(error) {\n            setError(null)\n            setIsLoading(true)\n        }\n        fetch(url)\n        .then(response => response.json())\n        .then(PokemonData => {\n            setNext(PokemonData.next)\n            if (PokemonData.previous === null) {\n                setPokemons(PokemonData.results)\n            } else {\n                setPokemons([...pokemons, ...PokemonData.results])\n            }\n        })\n        .catch(error => {\n            console.log(error)\n            setError('Error on load.')\n        })\n        .finally(() => {\n            setIsLoading(false)\n        })\n    }\n\n    useEffect(() => {\n        setIsLoading(true)\n\n        loadPokemons()\n    }, [url])\n\n    return {next, isLoading, error, pokemons, loadPokemons }\n\n}\n\nexport default useApiRequest;",{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","replacedBy":"68"},{"ruleId":"69","severity":1,"message":"70","line":1,"column":17,"nodeType":"71","messageId":"72","endLine":1,"endColumn":26},{"ruleId":"69","severity":1,"message":"73","line":7,"column":33,"nodeType":"71","messageId":"72","endLine":7,"endColumn":42},{"ruleId":"74","severity":1,"message":"75","line":25,"